int adjacent_duplicate_values(int[], int);

// function to find the first occurrence of adjacent duplicate values in the array

int adjacent_duplicate_values(int a[], int n)
{
    int i;
    printf("Enter %d elements : ", n);
    for (i = 0; i <= n - 1; i++)
    {
        scanf("%d", &a[i]);
    }
    for (i=0 ; i<=n-1 ; i++)
    {
        if (a[i] == a[i + 1])
        return a[i];
    }
}