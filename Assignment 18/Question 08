int count_words(char []);

// function to count words in a given string

int count_words(char str[])
{
    int i , count = 0 , n ;
    n = strlen(str)-1 ;
    for(i=0 ; i<n ; i++)
    {
        if(n>1)
        if(str[i]==' ')
        count++;
    }
    if(n>1)
    return count+1 ;
    else
    return 0 ;
}