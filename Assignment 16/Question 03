#include<stdio.h>
int main()
{
    int a[100][100] , b[100][100] , i , j , n1 , n2 ;
    printf("Enter number of rows of matrix : ");
    scanf("%d" ,&n1);
    printf("\nEnter number of columns of matrix : ");
    scanf("%d" ,&n2);
    printf("\nEnter the elements of matrix :\n");
    for(i=0 ; i<n1 ; i++)
    for(j=0 ; j<n2 ; j++)
    scanf("%d" , &a[i][j]);
    // transpose of matrix
    printf("\ntranspose of matrix is :\n");
    for(i=0 ; i<n2 ; i++)
    {
        for(j=0 ; j<n1 ; j++)
        {
            b[i][j] = a[j][i] ;
            printf("%d " , b[i][j]);
        }
        printf("\n");
    } 
}