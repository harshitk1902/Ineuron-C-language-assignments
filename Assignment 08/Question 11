#include<stdio.h>
int main()
{
    int i , j , a ;
    for (int i=1 ; i<=5 ; i++)
    {
        a = 'A' ;
        for (int j=1 ; j<=9 ; j++)
        {
            if(j>=(6-i) && j<=(4+i))
            {
            printf("%c",a);
            a++;
            if(i>1 && j>4)
            a = a-2 ;
            }
            else
            printf(" ");
        }
        printf("\n");
    }
    return 0 ;
}