#include<stdio.h>
int main()
{
    int i , j , a ;
    for (int i=1 ; i<=4 ; i++)
    {
        a = 'A' ;
        for (int j=1 ; j<=7 ; j++)
        {
            if(j>=i && j<=8-i)
            {
            printf("%c",a);
            a++;
            if(i>=1 && j>=4)
            a = a - 2 ;
            }
            else
            printf(" ");
        }
        printf("\n");
    }
    return 0 ;
}