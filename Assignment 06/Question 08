#include <stdio.h>
int main()
{
    int i , n , k=0 ;
    printf("Enter a number : ") ;
    scanf("%d" , &n) ;
    for(i=2 ; i<n ; i++)
    {
        if(n%i==0)
        {
            k++ ;
            break;
        }
    }
    if(k)
    printf("%d is not a prime number",n);
    else
    printf("%d is a prime number",n);
    return 0;
}
