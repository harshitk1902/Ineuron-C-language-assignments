#include<stdio.h>
int main()
{
    int i , a[10] , sum = 0 ;
    float average ;
    printf("Enter 10 numbers : ");
    for (i=0; i<=9; i++)
    {
        scanf("%d",&a[i]);
        sum = sum + a[i];
    }
    average = sum / 10.0 ;
    printf("Average is %f" , average);
    return 0 ;
}