#include <stdio.h>
int main()
{
    int i , j , a[99999] , n , temp , b[99999];
    printf("How many numbers you want to enter : ");
    scanf("%d", &n);
    printf("\nEnter the elements : ");
    for (i=0 ; i<=n-1 ; i++)
    {
        scanf("%d", &a[i]);
        b[i] = a[i] ;
    }
    printf("\nCopied elements of array are ");
    for (i=0 ; i<=n-1 ; i++)
    {
        printf("%d ", b[i]);
    }
    return 0;
}