#include<stdio.h>
int main () { 
int n , n1 = -1 , n2 = 1 , Nth ;
printf("Enter term for fibonacci series : ") ;
scanf("%d" , &n) ;
for(int i=1 ; i<=n ; i++)
{
    Nth = n1 + n2 ;
    n1 = n2 ;
    n2 = Nth ;
}
printf("The %dth term of fibonacci series is %d " , n , Nth) ;
return 0 ; 
}