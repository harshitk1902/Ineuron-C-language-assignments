#include <stdio.h>
int main()
{
int n , n1 = 0 , n2 = 1 , Nth ;
printf("Enter number of terms for fibonacci series : ") ;
scanf("%d" , &n) ;
printf("Required fibonacci series is %d %d " , n1 , n2) ;
for(int i=3 ; i<=n ; i++)
{
    Nth = n1 + n2 ;
    n1 = n2 ;
    n2 = Nth ;
    printf("%d " , Nth) ;
}
return 0 ; 
}