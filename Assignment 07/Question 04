#include <stdio.h>
int main()
{
    int x , y , i , HCF , limit ;
    printf("Enter two numbers : ") ;
    scanf("%d%d" , &x , &y) ;
    if(x<y)
    limit = x ;
    else 
    limit = y ;
    for (i = 1 ; i <= limit ; i++)
    {
        if( (x%i==0 && y%i==0) )
        {
        HCF = i ;
        }
    }
    printf("\nHCF is %d" , HCF);
    return 0;
}
