#include<stdio.h>
#include<string.h>
int main()
{
    int i , count = 0 ;
    char str[100000] ;
    printf("Enter a string : ");
    fgets(str,100000,stdin);
    count = strlen(str) - 1 ;
    printf("\nreversed string is : ");
    for(i=count-1 ; i>=0 ; i--)
    {
        printf("%c" , str[i]);
    }
    return 0;
}