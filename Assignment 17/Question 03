#include <stdio.h>
int main()
{
    int i , count = 0 ;
    char str[100000] , a ;
    printf("Enter a string : ");
    fgets(str,100000,stdin);
    for(i=0 ; str[i] ; i++)
    {
        if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u')
        count++;
        else if(str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')
        count++;
    }
    printf("\ntotal vowels are %d" , count);
    return 0;
}