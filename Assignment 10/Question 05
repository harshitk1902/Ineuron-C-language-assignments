#include<stdio.h>
#include<conio.h>
void N_odd_natural(int);
int main ()  
{
    int a ;
    printf("Enter a number : ") ;
    scanf("%d" , &a) ;
    N_odd_natural(a) ;
    getch() ;
}
void N_odd_natural(int n)
{
    printf("\nThe first %d odd natural numbers are :\n" , n);
    for (int i = 1 ; i<=2*n ; i++)
    {
        if(i%2!=0)
        printf("%d " , i) ;
    }
    printf("\n") ;
}