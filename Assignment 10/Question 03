#include<stdio.h>
#include<conio.h>
int even_odd(int);
// driver code
int main ()  
{
    int a , b ;
    printf("Enter a number : ") ;
    scanf("%d" , &a) ;
    b = even_odd(a) ;
    if(b==1)
    printf("\neven\n");
    else
    printf("\nodd\n");
    getch() ;
}
// function of even_odd
int even_odd(int n)
{
    if(n%2==0)
    return 1 ;
    else
    return 0 ;
} 